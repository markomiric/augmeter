# =============================================================================
coverage/
.nyc_output/
.coverage/
*.lcov
junit.xml
test-results/
test-reports/

# VS Code Test Runner
.vscode-test/
.vscode-test-web/

# =============================================================================
# Environment & Configuration
# =============================================================================
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
.env.*.local

# =============================================================================
# Logs
# =============================================================================
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*
.pnpm-debug.log*

# =============================================================================
# Runtime & Temporary Files
# =============================================================================
*.pid
*.seed
*.pid.lock
.tmp/
.temp/
*.tmp
*.temp
*.swp
*.swo
*~

